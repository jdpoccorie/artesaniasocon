@font-face {
	font-family: 'icomoon';
	src:url('../fonts3/icomoon.eot');
	src:url('../fonts3/icomoon.eot?#iefix') format('embedded-opentype'),
		url('../fonts3/icomoon.ttf') format('truetype'),
		url('../fonts3/icomoon.woff') format('woff'),
		url('../fonts3/icomoon.svg#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

section	

	.fotos
		
		background #f2f0f0
		padding 1.5em 0 0.5em 1em
		overflow hidden
		padding-bottom 3em		

		.titulo_fotos
			h2
				font-family "letra_corrida"
				font-size 1.8em

				span
						color #603917

		.las_fotos
			margin-left 5em

		.volver
			text-align center
			width 100%
			background #ccc
			padding 0.5em 0

			a
				color #903903
				font-size 1.3em							
				text-decoration none
				font-weight bold

				&:before 
					content "\e600"
					font-family "icomoon"
					font-size 2em
				
				&:hover
					color #0c57a4
					font-weight bold
					
